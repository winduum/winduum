{
  "name": "winduum",
  "version": "0.2.2-next.1",
  "type": "module",
  "main": "utils/tailwind.cjs",
  "module": "utils/tailwind.js",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build-prod": "vite build -c vite.config.prod.js",
    "eslint": "eslint 'src/**/*.js' --fix",
    "stylelint": "npx stylelint 'src/**/*.css' --fix",
    "tailwind": "rollup utils/tailwind.js --file utils/tailwind.cjs -f cjs --exports named"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "css-has-pseudo": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "postcss": "^8.4.25",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.0",
    "postcss-custom-media": "^10.0.0",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-stylistic": "^0.4.3",
    "tailwindcss": "^3.3.2",
    "fast-glob": "^3.3.0",
    "vite": "^4.4.2",
    "vue": "^3.3.4"
  },
  "files": [
    "index.js",
    "tailwind.config.cjs",
    "dist",
    "src",
    "utils"
  ],
  "exports": {
    ".": {
      "default": "./utils/tailwind.js",
      "require": "./utils/tailwind.cjs",
      "import": "./utils/tailwind.js"
    },
    "./main.css": "./dist/main.css",
    "./main-rgb.css": "./dist/main-rgb.css",
    "./tailwind.css": "./dist/tailwind.css",
    "./src/*": "./src/*",
    "./utils/*": "./utils/*",
    "./base/*": "./src/base/*",
    "./components/*": "./src/components/*",
    "./libraries/*": "./src/libraries/*",
    "./ui/*": "./src/ui/*"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winduum/winduum.git"
  }
}

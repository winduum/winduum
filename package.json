{
  "name": "winduum",
  "version": "3.0.0-next.1",
  "type": "module",
  "types": "types/index.d.ts",
  "main": "plugin/index.cjs",
  "module": "plugin/index.js",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build-prod": "vite build -c vite.config.prod.js",
    "eslint": "eslint",
    "eslint-fix": "eslint --fix",
    "stylelint": "npx stylelint '**/*.css'",
    "stylelint-fix": "npx stylelint '**/*.css' --fix",
    "plugin": "rollup plugin/index.js --file plugin/index.cjs -f cjs --exports named",
    "tsc": "tsc",
    "dts": "node dts.js",
    "publish-next": "npm publish --tag next"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@floating-ui/dom": "^1.7.4",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@stylistic/stylelint-config": "^4.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.23",
    "dts-buddy": "^0.6.2",
    "eslint": "^9.39.2",
    "fast-glob": "^3.3.3",
    "invokers-polyfill": "^0.6.1",
    "postcss": "^8.5.6",
    "postcss-custom-media": "^12.0.0",
